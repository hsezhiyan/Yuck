class Foo {
  var z;
  function init() {
    this.z = 3;
  }

  function foo() {
    return this.z;
  }
}

function meh(n) {
  class Bar extends Foo {
    function init() {
      this(Foo).init();
    }

    function bar() {
      this.z = this.z + n;
      return true;
    }
  }
  return new Bar();
}

var z = meh(3);
z.bar();
return z.foo();