class Foo {
  var z;
  function init() {
    this.z = 3;
  }

  function foo() {
    return this.z;
  }
}

function meh() {
  class Bar extends Foo {
    function init() {
      this(Foo).init();
    }

    function bar() {
      return 5;
    }
  }
  return new Bar();
}

var z = meh();
return z.foo();